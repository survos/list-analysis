{% extends "base.html.twig" %}

{% block body %}

    <ul id="download_log">
        <li>ONE</li>
        <li>two</li>
        <li>three</li>
    </ul>

    <table class="table">
        <tbody>
        {% for period in periods%}
            <tr>
                <td><a href="{{ path('app_message_period', period.routeParams) }}">{{ period.id }}</a></td>
                <td>{{ period.monthNumber }}</td>
                <td>{{ period.year}}</td>
                <td>{{ period.importedMessageCount}}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('download') }}
{% endblock %}