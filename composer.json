{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pgsql": "*",
        "ext-sqlite3": "*",
        "api-platform/api-pack": "dev-master",
        "cmen/google-charts-bundle": "dev-master",
        "easycorp/easyadmin-bundle": "*",
        "expertcoder/swiftmailer-send-grid-bundle": "^2.2",
        "fabpot/goutte": "^3.2",
        "friendsofsymfony/elastica-bundle": "^5.1@dev",
        "friendsofsymfony/user-bundle": "~2.0",
        "knplabs/knp-menu-bundle": "^2.2",
        "sensio/framework-extra-bundle": "^5.1",
        "sentry/sentry-symfony": "^2.2",
        "survos/landing-bundle": "*@dev",
        "symfony/asset": "4.2.*",
        "symfony/console": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "4.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "dev-master",
        "symfony/yaml": "*",
        "twbs/bootstrap": "^4.1"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Survos\\LandingBundlex\\": "../Survos/LandingBundle/src/",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Survos/LandingBundlex"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.2.*"
        }
    }
}
